#include <stdio.h>

int main() 
{
    int num, sum = 0;
    float average;
    printf("Enter five numbers:\n");

    for (int i = 0; i < 5; i++) 
    {
        scanf("%d", &num);
        sum += num;
    }

    average = sum / 5;

    printf("The average of the five numbers is: %.2f\n", average);

    return 0;
}
